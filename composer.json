{
    "name": "obrainwave/auth-fusion",
    "description": "Unified authentication package for Laravel supporting Sanctum, JWT, and Passport",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "authentication",
        "sanctum",
        "jwt",
        "passport",
        "api-auth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Akeem Olaiwola Salau",
            "email": "olaiwola.salau@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "symfony/process": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "suggest": {
        "laravel/sanctum": "Required for Sanctum driver",
        "tymon/jwt-auth": "Required for JWT driver",
        "laravel/passport": "Required for Passport driver"
    },
    "autoload": {
        "psr-4": {
            "Obrainwave\\AuthFusion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obrainwave\\AuthFusion\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obrainwave\\AuthFusion\\AuthFusionServiceProvider"
            ],
            "aliases": {
                "AuthFusion": "Obrainwave\\AuthFusion\\Facades\\AuthFusion"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

